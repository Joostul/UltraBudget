@model UltraBudget.Entities.Wallet

@{
    ViewBag.Title = "Wallet details";
}

<html>
<body>
    <h1>@Model.ToString()</h1>
    <div>
        Id: @Model.Id
    </div>
    <div>
        Name: @Model.Name
    </div>
    <div>
        Currency: @Model.Currency.Name
    </div>
    <div>
        UserId: @Model.UserId
    </div>
    <div>
        <a asp-controller="Home" asp-action="Index">Home</a>
    </div>
</body>
</html>